<Window x:Class="WpfApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <UniformGrid Columns="2">
        <Grid>
            <ListBox Name="ls_box"/>
            <Button Name="btn_clear" Content="clear" VerticalAlignment="Bottom" HorizontalAlignment="Right"/>
        </Grid>
        <UniformGrid Rows="10" Columns="4">
            <Button Name="btn_motion_analyse" Content="流程数据分析"/>
            <Button Name="btn_align_analyse" Content="对位数据分析"/>
            <Button Name="btn_memory_analyse" Content="内存数据分析"/>
        </UniformGrid>
    </UniformGrid>
</Window>
