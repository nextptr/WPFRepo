             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CVbox.Panel.ProjPage.View"
             xmlns:s="https://github.com/canton7/Stylet"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <WrapPanel>
        <Button Command="{s:Action btnTest}" CommandParameter="1" Width="auto" HorizontalAlignment="Stretch" Height="30" Content="CPP与动态链接库分离测试"/>
        <Button Command="{s:Action btnTest}" CommandParameter="2" Width="auto" HorizontalAlignment="Stretch" Height="30" Content="CPP与动态链接库混合测试"/>
