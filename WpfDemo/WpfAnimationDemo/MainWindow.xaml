<Window x:Class="WpfAnimationDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAnimationDemo"
        xmlns:panel="clr-namespace:WpfAnimationDemo.FanBlade"
        xmlns:tablemove="clr-namespace:WpfAnimationDemo.TableMove"
        mc:Ignorable="d"
        Title="MainWindow" WindowState="Maximized" Height="450" Width="800">
    <TabControl>
        <TabItem>
            <TabItem.Header>
                <Label Content="风扇动画" Height="35" VerticalContentAlignment="Center"/>
            </TabItem.Header>
            <panel:FanBladePanel/>
        </TabItem>
        <TabItem>
            <TabItem.Header>
                <Label Content="控件移动动画" Height="35" VerticalContentAlignment="Center"/>
            </TabItem.Header>
            <tablemove:TableMovePanel/>
        </TabItem>
    </TabControl>
</Window>
