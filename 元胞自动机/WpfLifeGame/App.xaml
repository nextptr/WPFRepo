<Application x:Class="WpfLifeGame.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WpfLifeGame"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resource/ToggleButton.xaml"/>
                <ResourceDictionary Source="/Resource/YellowToggleButton.xaml"/>
                <ResourceDictionary Source="/Resource/DataGrid.xaml"/>
                <ResourceDictionary Source="/Resource/RadioButton.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
